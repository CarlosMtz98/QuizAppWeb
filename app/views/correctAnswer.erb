<div class="question-box">
  <label>Progress:</label>
  <progress value="<%= @answeredQuestions %>" max="<%= @noOfQuestions %>">progress</progress>
  <img class="check-img" src="images/check.png" alt="">
  <div class="spacer"></div>
  <span class="correctAnswer">Correct Answer!</span>
  <a href="/nextQuestion">
    <button class="continueButton">
      Continue
    </button>
  </a>
</div>